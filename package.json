{
  "name": "freemannotes",
  "version": "0.4.6",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "npm run setup-db && ts-node-dev --respawn --transpile-only server/src/index.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/server/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio",
    "setup-db": "prisma db push --schema=prisma/schema.prisma",
    "postinstall": "node scripts/postinstall.js",
    "release:patch": "npm version patch -m \"chore(release): v%s\" && git push && git push --tags",
    "release:minor": "npm version minor -m \"chore(release): v%s\" && git push && git push --tags",
    "release:major": "npm version major -m \"chore(release): v%s\" && git push && git push --tags"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@prisma/client": "^5.10.0",
    "@tiptap/extension-collaboration": "^3.18.0",
    "@tiptap/extension-link": "^3.18.0",
    "@tiptap/extension-text-align": "^3.18.0",
    "@tiptap/extension-underline": "^3.18.0",
    "@tiptap/react": "^3.18.0",
    "@tiptap/starter-kit": "^3.18.0",
    "@types/dompurify": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "dompurify": "^3.3.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.3",
    "prisma": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.5",
    "vite": "^5.0.0",
    "ws": "^8.16.0",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
